    "strip-ansi": "^4.0.0",
    "walk-sync": "^0.3.3"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^1.5.0",
    "chai": "^4.1.2",
    "co": "^4.6.0",
    "ember-cli": "~3.4.3",
    "ember-cli-babel": "^7.1.0",
    "ember-cli-blueprint-test-helpers": "^0.19.1",
