    "ember-source": "~3.16.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^7.1.1",
    "mocha-eslint": "^6.0.0",
    "mocha-only-detector": "^1.0.0",
    "qunit-dom": "^1.1.0",
    "testdouble": "^3.13.1"
