    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "mocha-eslint": "^4.1.0",
    "mocha-only-detector": "^1.0.0",
    "qunit-dom": "^0.8.0"
