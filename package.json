{
  "name": "ember-cli-template-lint",
  "version": "2.0.2",
  "description": "Lint your handlebars templates.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ember-template-lint/ember-cli-template-lint.git"
  },
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://github.com/ember-template-lint/ember-cli-template-lint",
  "bugs": {
    "url": "https://github.com/ember-template-lint/ember-cli-template-lint/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
